A2."U_NumResol" as "AutNumAutorizacion", A1."BeginStr" as "AutPrefijo", A2."U_InitialNum" as "AutSecuenciaInicio", A2."U_LastNum" as "AutSecuenciaFinal"
From OINV A0
Inner Join NNM1 A1 On A0."Series" = A1."Series"
Inner Join "@FEDIAN_NUMAUTORI" A2 On A1."Series" = A2."Code"
Inner Join OADM A3 On 1 = 1
Inner Join OCRD A4 On A0."CardCode" = A4."CardCode"
Inner Join OCRY A5 On A4."Country" = A5."Code"
Inner Join "@HBT_MUNICIPIO" A6 On A4."U_HBT_MunMed" = A6."Code"
Inner Join OCRN A7 On A0."DocCur" = A7."CurrCode"
Inner Join (Select "DocEntry", Sum("LineTotal") as "TotalImporteBruto" From INV1 Group By "DocEntry") A8 On A0."DocEntry" = A8."DocEntry"
Inner Join (Select "DocEntry", Sum("BaseSum") as "BaseImpu" From INV4 Where "RelateType" = '1' Group By "DocEntry") A9 On A0."DocEntry" = A9."DocEntry"
Inner Join OCTG A10 On A0."GroupNum" = A10."GroupNum"
Inner Join INV1 A11 On A0."DocEntry" = A11."DocEntry"
Inner Join INV4 A12 On A11."DocEntry" = A12."DocEntry" And A11."LineNum" = A12."LineNum"
Inner Join OSTC A13 On A12."StcCode" = A13."Code"
Inner Join OITM A14 On A11."ItemCode" = A14."ItemCode"
Where A0."DocEntry" = {0}</value>
