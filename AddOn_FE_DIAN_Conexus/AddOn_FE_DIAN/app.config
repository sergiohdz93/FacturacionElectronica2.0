Case When CharIndex('-', A3."TaxIdNum") != 0 Then SubString(A3."TaxIdNum", CharIndex('-', A3."TaxIdNum")+1, Len(A3."TaxIdNum")) Else '' End "EmiDVIdentificacion",

Case When A4."U_HBT_TipEnt" = '2' Then '1' When A4."U_HBT_TipEnt" = '1' Then '2' End "CompradorTipoPersona", A4."U_HBT_TipDoc" as "CompradorTipoIdentificacion", 
Case When CharIndex('-', A4."LicTradNum") = 0 Then A4."LicTradNum" Else SubString(A4."LicTradNum", 1, CharIndex('-', A4."LicTradNum")-1) End "CompradorIdentificacion",
Case When CharIndex('-', A4."LicTradNum") != 0 Then SubString(A4."LicTradNum", CharIndex('-', A4."LicTradNum")+1, Len(A4."LicTradNum")) Else '' End "CompradorDVIdentificacion",
A4."U_SEI_FERegFis" as "CompradorTipoRegimen", 'ZZ' as "CompradorRespFiscal", 'ZZ' as "CompradorImpuesto", A4."CardName" as "CompradorRazonSocial",
A4."U_HBT_Nombres" as "CompradorPrimerNombre", '' as "CompradorSegundoNombre", IsNull(A4."U_HBT_Apellido1",'') + ' ' + IsNull(A4."U_HBT_Apellido2",'') as "CompradorApellidos",
A4."CardName" as "CompradorNombreCompleto", A4."Country" as "CompradorPais", A5."Name" as "CompradorNombrePais", A6."U_NomDepartamento" as "CompradorDepartamento",
A6."Code" as "CompradorCodDepartamento", A6."U_NomDepartamento" as "CompradorCiudad", A6."Code" as "CompradorCodCiudad", IsNull(A4."ZipCode",'') as "CompradorCodPostal",
A4."Address" as "CompradorDireccion", 'True' as "CompradorEnviarCorreo", IsNull(A4."Phone1",'') as "CompradorTelefonoCont", IsNull(A4."E_Mail",'') as "CompradorCorreoElectronico",

A2."U_DocDIAN" as "FacTipoFactura", IsNull(A0."U_SEI_FETipOper",'') as "FacCodOperacion", 
CONVERT(char(10), A0."DocDate",126) + ' ' + 
CASE WHEN Len (Cast(A0."CreateTS" as nVarchar(6))) = 6 THEN  SUBSTRING(Cast(A0."CreateTS" as nVarchar(6)), 0,2) WHEN Len (Cast(A0."CreateTS" as nVarchar(6))) = 5 THEN  SUBSTRING ( Cast(A0."CreateTS" as nVarchar(6)), 0,1)
END +':'+
CASE WHEN Len (Cast(A0."CreateTS" as nVarchar(6))) = 6 THEN  SUBSTRING ( Cast(A0."CreateTS" as nVarchar(6)), 3,2) WHEN Len (Cast(A0."CreateTS" as nVarchar(6))) = 5 THEN  SUBSTRING ( Cast(A0."CreateTS" as nVarchar(6)), 2,2)
END +':'+
CASE WHEN Len (Cast(A0."CreateTS" as nVarchar(6))) = 6 THEN  SUBSTRING ( Cast(A0."CreateTS" as nVarchar(6)), 5,2) WHEN Len (Cast(A0."CreateTS" as nVarchar(6))) = 5 THEN  SUBSTRING ( Cast(A0."CreateTS" as nVarchar(6)), 4,2)  
END "FacFechaHoraFactura",

A7."ISOCurrCod" as "FacCodMoneda", A8."TotalImporteBruto" as "FacTotalImporteBruto", '0' as "FacTotalCargos", A0."DiscSum" as "FacTotalDescuentos", 
A9."BaseImpu" as "FacTotalBaseImponible", A8."TotalImporteBruto" + A0."VatSum"  as "FacTotalBrutoMasImp", '0' as "FacTotalAnticipos", A0."DocTotal" as "FacTotalFactura",

Case When A10."ExtraMonth" + A10."ExtraDays" &lt;= 0 Then '1' Else '2' End "FacMetodoPago", A0."U_SEI_FEMedPago" as "FacFormaPago", 
CONVERT(char(10), A0."DocDueDate",126) as "FacVencimientoFac",

A11."VisOrder"+1 as "DetFacConsecutivo", A11."ItemCode" as "Codigo", A11."ItemCode" as "CodigoEstandar", A14."U_SEI_FEIdent" as "Codificacion",
A11."Dscription" as "Descripcion", IsNull(A11."Quantity",'0') as "Cantidad",
CASE 
WHEN IsNull(A11."UomCode",'') = 'Manual' Then IsNull((Select IsNull(P0."U_DIAN_UM",'') From "@FEDIAN_HOMOL_UM" P0 Where P0."U_SAP_UM" = A11."unitMsr"),'') 
Else IsNull((Select IsNull(P0."U_DIAN_UM",'') From "@FEDIAN_HOMOL_UM" P0 Where P0."U_SAP_UM" = A11."UomCode"),'') End "UnidadMedida",
A11."Price" as "PrecioUnitario", A11."Price" as "PrecioSinImpuestos", A11."LineTotal"+A11."VatSum" as "PrecioTotal",

A11."VisOrder"+1 as "Secuencia", A13."U_SEI_FETributo" as "CodigoImpuesto", A13."Name" as "NombreImpuesto", 'False' as "EsRetencionImpuesto",
A12."BaseSum" as "BaseImponible", Cast(A12."TaxRate" as decimal(19,2)) as "Porcentaje", A12."TaxSum" as "ValorImpuesto",

