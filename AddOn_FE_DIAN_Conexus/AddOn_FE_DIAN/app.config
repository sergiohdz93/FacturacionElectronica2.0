            <setting name="FacturaVenta" serializeAs="String">
                <value>Select 
'FAC' as "TipoDocumento", '92063fe528f34ab4add59a441822e7e1' as "GuidEmpresa", '3b3cca1a9b124bffb82e0003dafb1616' as "GuidOrigen", 
'77e7c1be44b8e0340815fe688270bcf28cac68dbd15e334f6a830f22cf09bbff' as "HashSeguridad", IsNull(A2."U_ClaveTec",'') as "ClaveTecnica",
A0."DocEntry" as "CodigoErp", IsNull(A1."BeginStr",'') + Cast(A0."DocNum" as nVarchar(100)) as "NumeroDocumento",

Case When A3."U_HBT_TipEnt" = '2' Then '1' When A3."U_HBT_TipEnt" = '1' Then '2' End "EmiTipoPersona", A3."U_SEI_FETipDoc" as "EmiTipoIdentificacion",
